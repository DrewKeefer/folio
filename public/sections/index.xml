      <guid>http://localhost:1313/folio/sections/digital/</guid>
      <description>&lt;p&gt;This is the digital work page&lt;/p&gt;&#xA;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;    &#xD;&#xA;    &lt;div class=&#34;image-grid-container&#34;&gt;&#xD;&#xA;        &lt;div class=&#34;selector-column&#34;&gt;&#xD;&#xA;            &#xD;&#xA;            &#xD;&#xA;                &#xD;&#xA;                &#xD;&#xA;&#xD;&#xA;                &#xD;&#xA;                    &#xD;&#xA;                &#xD;&#xA;                &lt;button &#xD;&#xA;                    class=&#34;thumbnail-button gallery&#34;&#xD;&#xA;                    data-src=&#34;http://localhost:1313/folio/digital/digit_01-01_hu_6cde8c65223691b7.png&#34;&#xD;&#xA;                    data-title=&#34;Geometric MoGraph&#34;&#xD;&#xA;                    data-caption=&#34;procedural motion graphics with hard cuts&#34;&#xD;&#xA;                    onmouseenter=&#34;makeActive(this)&#34;&#xD;&#xA;                    onfocus=&#34;makeActive(this)&#34;&#xD;&#xA;                &gt;&#xD;&#xA;                    Geometric MoGraph&#xD;&#xA;                &lt;/button&gt;&#xD;&#xA;            &#xD;&#xA;                &#xD;&#xA;                &#xD;&#xA;&#xD;&#xA;                &#xD;&#xA;                &lt;button &#xD;&#xA;                    class=&#34;thumbnail-button gallery&#34;&#xD;&#xA;                    data-src=&#34;http://localhost:1313/folio/digital/digit_02-01_hu_8788f8263524498e.png&#34;&#xD;&#xA;                    data-title=&#34;Glitch Experiments&#34;&#xD;&#xA;                    data-caption=&#34;Datamosh passes, temporal tearing&#34;&#xD;&#xA;                    onmouseenter=&#34;makeActive(this)&#34;&#xD;&#xA;                    onfocus=&#34;makeActive(this)&#34;&#xD;&#xA;                &gt;&#xD;&#xA;                    Glitch Experiments&#xD;&#xA;                &lt;/button&gt;&#xD;&#xA;            &#xD;&#xA;        &lt;/div&gt;&#xD;&#xA;        &lt;div class=&#34;preview-column&#34;&gt;&#xD;&#xA;            &#xD;&#xA;            &#xD;&#xA;            &lt;a id =&#34;preview-link&#34; class = &#34;lightbox-image gallery&#34; href= &#34;/folio/digital/digit_01-01.png&#34;&gt;&#xD;&#xA;                &lt;img id=&#34;preview-image&#34; src=&#34;http://localhost:1313/folio/digital/digit_01-01_hu_6cde8c65223691b7.png&#34; alt=&#34;&#34;&gt;&#xD;&#xA;            &lt;/a&gt;&#xD;&#xA;            &lt;div class=&#34;preview-text&#34;&gt;&#xD;&#xA;                &lt;h2 id=&#34;preview-title&#34;&gt; Geometric MoGraph&lt;/h2&gt;&#xD;&#xA;                &lt;p id=&#34;preview-caption&#34;&gt; procedural motion graphics with hard cuts&lt;/p&gt;&#xD;&#xA;            &lt;/div&gt; &#xD;&#xA;        &lt;/div&gt;&#xD;&#xA;    &lt;/div&gt;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;&lt;script&gt;&#xD;&#xA;    const previewLink = document.getElementById(&#34;preview-link&#34;)&#xD;&#xA;    const previewImage = document.getElementById(&#34;preview-image&#34;);&#xD;&#xA;    const previewTitle = document.getElementById(&#34;preview-title&#34;)&#xD;&#xA;    const previewCaption = document.getElementById(&#34;preview-caption&#34;)&#xD;&#xA;&#xD;&#xA;    function makeActive(button) {&#xD;&#xA;        const src = button.getAttribute(&#34;data-src&#34;);&#xD;&#xA;        const title = button.getAttribute(&#34;data-title&#34;);&#xD;&#xA;        const caption = button.getAttribute(&#34;data-caption&#34;);&#xD;&#xA;&#xD;&#xA;        previewLink.href = src;&#xD;&#xA;        previewLink.title = title;&#xD;&#xA;        previewImage.src = src;&#xD;&#xA;        previewImage.alt = title;&#xD;&#xA;        previewTitle.innerText = title;&#xD;&#xA;        previewCaption.innerText = caption;&#xD;&#xA;    };&#xD;&#xA;&lt;/script&gt;</description>
